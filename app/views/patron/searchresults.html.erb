<h1>Found Books</h1>
<table>
<tr>
<th>Title</th>
<th>Author</th>
<th>Isbn</th>
<th></th>
</tr>
<%	@books.each do	|book|	%>
<tr>
<td><%=	book.title %></td>
<td><%=	book.author %></td>
<td><%=	book.isbn %></td>
<td><%=	link_to 'Add review',	new_book_review_path(book)	%></td>
<td><%=	link_to 'Reviews',	book_path(book)	%></td>
</tr>
<%	end	%>
</table>
<p>
  <%=	link_to 'Back',	patron_search_path %>
</p>
